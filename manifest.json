{
  "name": "wysiwyg-textarea",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "You can edit textarea under wysiwyg mode",
  "content_scripts": [
    {
      "matches": [
        "https://gitlab.com/*/wikis/*/edit",
        "https://gitlab.com/*/wikis/*?random_title=true"
      ],
      "js": [
        "jquery.min.js",
        "simplemde.min.js",
        "highlight.min.js",
        "script.js"
      ],
      "css": ["simplemde.min.css", "github.min.css"],
      "run_at": "document_start"
    }
  ]
}
